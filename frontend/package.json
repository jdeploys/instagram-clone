{
  "name": "insta-clone",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.3.0",
    "@loadable/component": "^5.15.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "axios": "^0.23.0",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-infinite-scroll-component": "^6.1.0",
    "react-responsive-carousel": "^3.2.22",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "react-toastify": "^8.0.3",
    "typescript": "^4.1.2",
    "unstated-next": "^1.1.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "dev": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "ts-check": "tsc --skipLibCheck --noEmit",
    "gen": "dtsgen --url https://jsbackend.herokuapp.com/swagger/openapi.json --out src/@types/index.d.ts && prettier --write \"./src/@types/index.d.ts\"",
    "gen-api": "npx swagger-typescript-api -p https://jsbackend.herokuapp.com/swagger/openapi.json -o ./src/api --axios --modular --single-http-client  --extract-request-params --extract-request-body"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.3.1",
    "autoprefixer": "^9",
    "craco-alias": "^3.0.1",
    "postcss": "^7",
    "prettier": "^2.4.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  }
}
